<div data-role="page" data-add-back-btn="false">

  <div data-role="header" data-position="inline">
    <h1><%= @schedule.user_id %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="arrow-l" data-direction="reverse">
      Back
    </a>
    <a href="<%= url_for :action => :edit, :id => @schedule.object %>" class="ui-btn-right" data-icon="star">
      Edit
    </a>
  </div>

  <div data-role="content">
    <ul data-role="listview">
      
          <li>
            <div class="itemLabel">UserId</div>
            <div class="itemValue"><%= @schedule.user_id %></div>
          </li>
      
          <li>
            <div class="itemLabel">CompanyId</div>
            <div class="itemValue"><%= @schedule.company_id %></div>
          </li>
      
          <li>
            <div class="itemLabel">IntervieweeId</div>
            <div class="itemValue"><%= @schedule.interviewee_id %></div>
          </li>
      
          <li>
            <div class="itemLabel">Remark</div>
            <div class="itemValue"><%= @schedule.remark %></div>
          </li>
      
          <li>
            <div class="itemLabel">ScheduleTime</div>
            <div class="itemValue"><%= @schedule.schedule_time %></div>
          </li>
      
          <li>
            <div class="itemLabel">Status</div>
            <div class="itemValue"><%= @schedule.status %></div>
          </li>
      
    </ul>
  </div>

</div>
